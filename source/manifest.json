{
  "manifest_version": 2,

  "name": "ecoify Plugin",
  "description": "This extension will eco-ify your browsing behaviour.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "assets/icon.png",
   "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
   "activeTab",
   "webRequest",
   "storage",
   "webRequestBlocking",
   "*://*.google.com/*",
   "*://*.google.de/*"
   ]
}
